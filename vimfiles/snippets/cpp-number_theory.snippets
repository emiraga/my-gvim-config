snippet genprimes
	#define MAXP 1000000
	bool primes[MAXP];
	vector<int> plist;

	void genprimes() 
	{
		plist.clear();
		int m = (int)sqrt(int(MAXP));
		primes[0]=primes[1]=0;
		for(int i=2;i<MAXP;i++) primes[i] = 1;
		for(int i=2;i<=m;i++)
			if (primes[i])
				for(int j=i*i;j<MAXP;j+=i) primes[j] = 0;
		for(int i=2;i<MAXP;i++)if(primes[i])plist.push_back(i);
	}

